<form method="post" action="{{ path('app_sessions_delete', {'id': session.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cette session ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ session.id) }}">
    <button class="btn"><i class="fa-solid fa-trash-can text-black "></i></button>
</form>
