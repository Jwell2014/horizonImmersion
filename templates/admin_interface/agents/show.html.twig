{% extends 'base_ju.html.twig' %}

{% block title %}User{% endblock %}

{% block body %}
    <h1>User</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ user.username }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ user.roles ? user.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user.password }}</td>
            </tr>
            <tr>
                <th>Session_id</th>
                <td>{{ user.sessionId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_agents_index') }}">back to list</a>

    <a href="{{ path('app_agents_edit', {'id': user.id}) }}">edit</a>

    {{ include('admin_interface/agents/_delete_form.html.twig') }}
{% endblock %}
