<form method="post" action="{{ path('document_delete', {'id': document.id}) }}" onsubmit="return confirm('Etes-vous sure de vouloir supprimer ce document?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ document.id) }}">
    <button class="btn btn btn-danger deleteCateg">Delete</button>
</form>
