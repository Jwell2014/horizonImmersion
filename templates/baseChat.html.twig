<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        <link href="{{ asset('css/style.css') }}" rel="stylesheet"/>
        <link href="https://fonts.googleapis.com/css2?family=Jost:wght@300;500;700&display=swap&family=Material+Icons" rel="stylesheet">
        <!-- <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/webkit.js/0.1.0/webkit.js" rel="stylesheet">

    {% endblock %}


</head>
<body>
{% block body %}{% endblock %}
</body>

{% block javascripts %}

    <script src="/socket.io/socket.io.js"></script>
    <script src="{{ asset('js/main.js') }}"></script>
    <script src="{{ asset('js/index.js') }}"></script>

{% endblock %}
</html>
